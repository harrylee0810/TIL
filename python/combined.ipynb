     ]
    }
   ],
   "source": [
    "# 아래에 반복문을 활용하여 만들어주세요.\n",
    "girls = ['jane', 'iu', 'mary']\n",
    "boys = ['justin', 'david', 'kim']\n",
    "\n",
    "result = []\n",
    "for i in girls:\n",
    "    for k in boys:\n",
    "        couple = (i,k)\n",
    "        result.append(couple)\n",
